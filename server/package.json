{
  "name": "SocketLogger",
  "version": "0.0.1",
  "description": "Common Remote Logging",
  "main": "main_process/main.js",
  "scripts": {
    "start": "electron .",
    "watch": "webpack --env.patch --colors --progress --watch",
    "packageall": "electron-packager ./ --platform=win32,linux --arch=x64 --out=../dist --overwrite --icon=ansysicon",
    "packagelinux": "electron-packager ./ --platform=linux --arch=x64 --out=../dist --overwrite --icon=ansysicon",
    "packagewindows": "electron-packager ./ --platform=win32 --arch=x64 --out=../dist --overwrite --icon=ansysicon"
  },
  "keywords": [
    "logging",
    "sockets",
    "gui",
    "utility"
  ],
  "author": "Jake Callery",
  "devDependencies": {
    "electron-packager": "electron-packager@10.1.0",
    "babel-core": "babel-core@6.26.0",
    "babel-loader": "babel-loader@7.1.2",
    "babel-preset-es2015": "babel-preset-es2015@6.24.1",
    "bluebird": "bluebird@3.5.1",
    "chalk": "chalk@2.3.0",
    "clean-webpack-plugin": "clean-webpack-plugin@0.1.17",
    "css-loader": "css-loader@0.28.7",
    "file-loader": "file-loader@1.1.6",
    "html-loader": "html-loader@0.5.1",
    "html-webpack-plugin": "html-webpack-plugin@2.30.1",
    "imagemin-webpack-plugin": "imagemin-webpack-plugin@1.5.2",
    "semver": "semver@5.4.1",
    "style-loader": "style-loader@0.19.1",
    "typings": "typings@2.1.1",
    "url-loader": "url-loader@0.6.2",
    "webpack": "webpack@2.3.3"
  },
  "dependencies": {
    "babel-polyfill": "babel-polyfill@6.26.0",
    "electron": "electron@1.8.1",
    "electron-json-storage": "electron-json-storage@4.0.2",
    "shortid": "shortid@2.2.8",
    "winston": "winston@2.4.0",
    "document-ready-promise": "document-ready-promise@3.0.1",
    "webpack": "webpack@2.3.3",
    "whatwg-fetch": "whatwg-fetch@2.0.3"
  }
}